{
  "name": "neat",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "api:dev": "turbo run api#dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "start:db": "turbo run start:db",
    "shut:db": "turbo run shut:db",
    "migrate:up": "turbo run migrate:up",
    "migrate:down": "turbo run migrate:down"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "syncpack": "14.0.0-alpha.19",
    "turbo": "^2.5.5",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=21"
  }
}
